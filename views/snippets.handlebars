<!DOCTYPE html>
<html lang="en">
  {{> head}}

  <body onload="smartquotes();">
    <div class="container">
      {{> topRow}}

      <div class="row">
        <div class="col-md-12">
          <h1 class="text-center">Sch√∂nfinkel Snippets</h1>
        </div>
      </div>

      <div class="row">
        <div class="col-md-12">
          <h2 class="text-center text-muted">As written by other users</h2>
        </div>
      </div>

      <div class="row" style="padding-top: 2rem; padding-bottom: 2rem;">
        {{#each snippetData}}
          {{#if (multThree @index)}}
            </div>

            <div class="row">
          {{/if}}

{{> snippet}}
        {{/each}}
      </div>

      {{> footer}}
    </div>

    {{> jsFooter}}
  </body>
</html>
